{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Remote Gin Debugger",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "port": 9229,
      "host": "127.0.0.1",
      "showLog": true,
      "trace": "log",
      "remotePath": "/app",
      "cwd": "${workspaceFolder}/backend",
      "substitutePath": [
        {
          "from": "${workspaceFolder}/backend",
          "to": "/app"
        }
      ]
    }
  ]
}
